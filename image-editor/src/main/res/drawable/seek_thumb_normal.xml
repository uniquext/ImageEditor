<?xml version="1.0" encoding="utf-8"?><!--<shape xmlns:android="http://schemas.android.com/apk/res/android"--><!--android:shape="oval">--><!--<solid android:color="@color/seek_thumb_normal" />--><!--<stroke--><!--android:width="1dp"--><!--android:color="#a3ce98" />--><!--<size--><!--android:width="20dp"--><!--android:height="20dp" />--><!--</shape>-->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:top="2dp">
        <shape android:shape="oval">
            <size
                android:width="22dp"
                android:height="22dp" />
            <gradient
                android:endColor="#00000000"
                android:gradientRadius="11dp"
                android:startColor="#000000"
                android:type="radial" />
        </shape>
    </item>

    <item android:bottom="2dp">
        <shape android:shape="oval">
            <solid android:color="@color/seek_thumb_normal" />
            <size
                android:width="20dp"
                android:height="20dp" />
        </shape>
    </item>
</layer-list>